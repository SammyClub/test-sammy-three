# Sammy 3 Configuration Template
# Copy this file to .env and fill in your values

# Required: JWT token for authentication
REACT_APP_JWT_TOKEN=your_jwt_token_here

# Optional: Guide ID for specific walkthroughs
REACT_APP_GUIDE_ID=

# API Configuration
REACT_APP_SAMMY_API_URL=https://api.sammylabs.com

# Capture quality (0.0 to 1.0, default: 0.9)
REACT_APP_CAPTURE_QUALITY=0.9

# Debug Configuration
# Enable debug logs
REACT_APP_DEBUG_LOGS=false

# Enable audio performance debugging
REACT_APP_DEBUG_AUDIO=false

# Enable worker mode (automatic in production)
REACT_APP_USE_WORKER_MODE=true

# Advanced Features (Sammy 3)
# Enable observability tracking
REACT_APP_ENABLE_OBSERVABILITY=true

# Enable MCP (Model Context Protocol) support
REACT_APP_ENABLE_MCP=false

# Development Settings
NODE_ENV=development

